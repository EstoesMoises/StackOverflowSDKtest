{
  "name": "so-teams-sdk",
  "version": "1.2.0",
  "description": "SDK for Stack Overflow Teams API",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "scripts": {
    "generateSDK": "openapi-generator-cli generate -c generator-settings.json",
    "build": "pnpm generateSDK && pnpm compile",
    "compile": "tsc",
    "clean": "rm -rf dist/",
    "prepublishOnly": "pnpm clean && pnpm build",
    "test": "npx tsx ./test.ts",
    "sdk:check": "node automation/sdk-updater.mjs check",
    "sdk:update": "node automation/sdk-updater.mjs run",
    "sdk:status": "node automation/sdk-updater.mjs status",
    "sdk:dry-run": "DRY_RUN=true node automation/sdk-updater.mjs run",
    "update-sdk": "node automation/sdk-updater.mjs"
  },
  "keywords": [
    "stackoverflow",
    "teams",
    "api",
    "sdk"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "dotenv": "^17.2.1",
    "es6-promise": "^4.2.4",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.21.0",
    "@types/node": "^24.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "@octokit/rest": "^22.0.0",
    "openai": "^5.21.0"

  }
}
